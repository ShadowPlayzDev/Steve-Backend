{
  "rewrites": [
    { "source": "/(.*)", "destination": "/pages/$1/page.html" },
    { "source": "/", "destination": "/index.html" },
    { "source": "/dsc-readme", "destination": "/dsc-readme.txt" }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    }
  ]
}
